{
  "name": "musictracker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon src/server/main.ts -w src/server",
    "start": "NODE_ENV=production ts-node src/server/main.ts",
    "start:backend": "NODE_ENV=production ISOLATION=backend ts-node src/server/main.ts",
    "start:frontend": "NODE_ENV=production ISOLATION=frontend ts-node src/server/main.ts",
    "build": "vite build",
    "generate-migrations": "drizzle-kit generate:sqlite",
    "migrate": "drizzle-kit push:sqlite",
    "test:unit": "vitest ./src"
  },
